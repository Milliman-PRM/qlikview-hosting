<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  
  <connectionStrings>
    <add name="PWUdbContextConnectionString" connectionString="Server=.\SQLExpress;Database=PortalDBSFv2M1;Trusted_Connection=Yes;User Instance=False;" />
  </connectionStrings>
  <!-- Membership Provider Setup -->
  <system.web>
    <membership defaultProvider="dbSqlMemberShipProvider" userIsOnlineTimeWindow="15">
      <providers>
        <clear />
        <add name="dbSqlMemberShipProvider" 
             type="System.Web.Security.SqlMembershipProvider" 
             connectionStringName="PWUdbContextConnectionString" 
             enablePasswordRetrieval="true" 
             enablePasswordReset="true" 
             requiresQuestionAndAnswer="false" 
             applicationName="MyCMS" 
             requiresUniqueEmail="true" 
             passwordFormat="Encrypted" 
             maxInvalidPasswordAttempts="5" 
             minRequiredPasswordLength="7" 
             minRequiredNonalphanumericCharacters="1" 
             passwordAttemptWindow="10" 
        passwordStrengthRegularExpression="" />
        <!-- IMPORTANT!applicationName -->
        <!-- The membership provider "applicationName" property should be set to the [ApplicationName] column value from [aspnet_Applications] table  -->
      </providers>
    </membership>
    <!-- IMPORTANT!machineKey -->
    <!-- Because the password format is set to ENCRYPTED, a machine key like below is required. If you don't want to use the one provided below, you can create your own key at: http://www.eggheadcafe.com/articles/GenerateMachineKey/GenerateMachineKey.aspx. If you set the password format to Clear or Hashed, you can delete or comment out this machine key. -->
    <machineKey decryption="AES" 
                validation="SHA1" 
                decryptionKey="C7C731D64AAE26CDC6691216C00F37EF19016FE8447ACCF0" 
                validationKey="92F9A64F420F2E190F97BC1BA1B6E5329F97C4ED06BE19CB1932F39FBABE9C8B07685B2014DD6F37EDDBCC44A316085CACDC8C4727699D28B42F6031DBF158D6" />
  </system.web>
  <!-- appsettings_Dev -->
  <appSettings configSource="appsettings_Dev.config" />
</configuration>